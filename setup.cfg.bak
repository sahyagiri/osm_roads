[metadata]
name = osm_roads
version = 0.21
author = Gireesan Namboothiri
author_email = sahyagiri@gmail.com
description = 
long_description = file: README.md
long_description_content_type = text/markdown
url = https://github.com/sahyagiri/osm_roads
project_urls =
    Bug Tracker = https://github.com/sahyagiri/osm_roads/issues
classifiers =
    Programming Language :: Python :: 3
    License :: OSI Approved :: MIT License
    Operating System :: OS Independent
keywords = openstreetmap,map matching, pbf, trie, geohash

[options]
install_requires=
	datrie
	geojson
	osmium
	python_geohash
	turfpy
	
package_dir =
    = src
packages = find:
python_requires = >=3.6

[options.packages.find]
where = src
